		{
			"name": "Autenticación",
			"item": [
				{
					"name": "Registro",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"usuario@example.com\",\n  \"username\": \"usuario123\",\n  \"password\": \"password123\"\n}"
						},
						"url": {
							"raw": "{{URL}}/api/auth/register",
							"host": ["{{URL}}"],
							"path": ["api", "auth", "register"]
						},
						"description": "Registra un nuevo usuario en el sistema"
					}
				},
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    pm.environment.set(\"auth_token\", jsonData.token);",
									"}"
								]
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"usuario@example.com\",\n  \"password\": \"password123\"\n}"
						},
						"url": {
							"raw": "{{URL}}/api/auth/login",
							"host": ["{{URL}}"],
							"path": ["api", "auth", "login"]
						},
						"description": "Inicia sesión y obtiene un token JWT"
					}
				}
			]
		},
